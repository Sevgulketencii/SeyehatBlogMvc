@model TravelMvc.Models.Sınıflar.BlogYorum


<div class="con-bot">
    @foreach (var item in Model.deger4)
    {
        <div class="left">
            <div class="bot-left">
                <img src=@item.BlogImg alt="" />
            </div>
            <div class="left-text">
                <h4>@item.Baslik</h4>
                <span class="doller">$1024.99</span>
                <p>@item.Aciklama.Substring(0, 150)</p>
                <p>@item.Tarih.ToShortDateString()</p>

                <p><a href="#"><span class="read">Read More >></span></a></p>
            </div>
        </div>


    }

    @Html.Partial("Partial2")

    <div class="clear"></div>
</div>