@model TravelMvc.Models.Sınıflar.BlogYorum

<div class="right">
    @foreach (var item in Model.deger3)
    {
        <div class="bot-left">
            <img src=@item.BlogImg alt="" />
        </div>
        <div class="left-text">

            <h4>@item.Baslik</h4>
            <span class="doller">$924.99</span>
            <p>@item.Aciklama.Substring(0,150)</p>
            <p>@item.Tarih</p>
            <p><a href="#"><span class="read">Read More >></span></a></p>
        </div>
    }

</div>